{
  "name": "csi.lk",
  "description": "Callum Silcock's personal website",
  "version": "1.0.0",
  "author": "Callum Silcock",
  "dependencies": {
    "gatsby": "^1.9.183",
    "gatsby-link": "^1.6.36",
    "gatsby-plugin-netlify-cms": "^1.0.2",
    "gatsby-plugin-react-helmet": "^1.0.5",
    "gatsby-plugin-sass": "^1.0.16",
    "gatsby-plugin-sharp": "^1.6.27",
    "gatsby-remark-images": "^1.5.41",
    "gatsby-source-filesystem": "^1.5.18",
    "gatsby-transformer-remark": "^1.7.30",
    "gatsby-transformer-sharp": "^1.6.18",
    "normalize.scss": "^0.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-load-script": "0.0.6"
  },
  "keywords": [
    "callum",
    "silcock",
    "portfolio"
  ],
  "license": "UNLICENSED",
  "main": "n/a",
  "scripts": {
    "start": "npm run develop",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write src/**/*.js",
    "test": "echo \"ðŸ›‚  Running Tests with Coverage:\" && jest --coverage",
    "lint": "echo \"ðŸ”§  Running Linters:\" && run-p lint:js",
    "lint:js": "eslint --ignore-path .gitignore ./src ./tools --color",
    "lint:js:fix": "eslint --ignore-path .gitignore ./src ./tools --fix"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.1.4",
    "jest-junit": "^3.5.0",
    "prettier": "^1.10.2"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/"
    ],
    "collectCoverageFrom": [
      "src/**/**/*.{js}",
      "!.cache"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "jest-junit": {
    "suiteName": "mmgp-user-feedback tests",
    "output": "./coverage/junit.xml"
  }
}
